int main(){
    int arr[10000];
    int stack[10000];
    int n;
    cin >> n;
    int p, r;
    p = 0;
    r = n - 1;
    int i, j, temp;
    for(i = 0; i < n; i = i + 1){
        cin >> temp;
        arr[i] = temp;
    }
    if (p < r){
        int pointer;
        pointer = -1;
        pointer = pointer + 1;
        stack[pointer] = p;
        pointer = pointer + 1;
        stack[pointer] = r;
        int ex, left, right;
        ex = 0;
        left = 0;
        right = 0;
        while(pointer != -1){
            right = stack[pointer];
            pointer = pointer - 1;
            left = stack[pointer];
            pointer = pointer - 1;
            i = left - 1;
            int pivot;
            pivot = arr[right];
            for (j = left; j < right; j = j + 1){
                if (arr[j] < pivot){
                    i = i + 1;
                    ex = arr[i];
                    arr[i] = arr[j];
                    arr[j] = ex;
                }
            }
            arr[right] = arr[i + 1];
            arr[i + 1] = pivot;
            if(left < i){
                pointer = pointer + 1;
                stack[pointer] = left;
                pointer = pointer + 1;
                stack[pointer] = i;
            }
            if(i + 2 < right){
                pointer = pointer + 1;
                stack[pointer] = i + 2;
                pointer = pointer + 1;
                stack[pointer] = right;
            }        
        }
        for (i = 0; i < r; i = i + 1){
            stack[i] = 0;
        }
    }
    for (i = 0; i < n; i = i + 1){
        cout << arr[i] << endl;
    }
    
    return 0;
}