int main(){
    int a, b, i;
    a = 3;
    if(a == 1){
        a = a + 1;
    }
    else if(a == 2){
        a = a + 2;
    }
    else{
        b = a + 3;
    }
    while(a < 10){
        cout << a;
        b = b + 3;
        a = a + 1;
    }
    for(i = 0; i < 10; i = i + 1){
        cout << i;
    }
    cout << a;
    cout << b;
    cout << a[2];
