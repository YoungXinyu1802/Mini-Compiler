int main(){
    char a[10000];
    int flag, i;
    flag = 1;
    i = 0;
    while(flag){
        char temp;
        cin >> temp;
        if(temp != endl){
            a[i] = temp;
        }
        else{
            flag = 0;
        }
        cout << a[i] << endl;
        i = i + 1;
    }
    return 0;
}