void ff(int b,int array[]){
    cout << "ff" << endl;
    cout << b << endl;
    cout << "array" << array[1] << endl;
    array[1] = 3;
}
int main(){
    int a[5];
    a[5] = {1, 2, 3, 4, 5};
    int i;
    ff(1,a[]);
    a[0]=9;
    i=a[1];
    a[i]=0;
    cout<<a[0];
    cout<<a[i];
    cout<<endl;
    cout << a[1] << endl;
    return 0;
}